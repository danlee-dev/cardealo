# Database (Railway PostgreSQL - auto-provided by Railway)
# If not set, falls back to local SQLite (cardealo.db)
# DATABASE_URL=postgresql://user:password@host:port/database

# Naver Cloud Platform (Maps - Geocoding, Reverse Geocoding)
NCP_CLIENT_ID=your_ncp_client_id
NCP_CLIENT_SECRET=your_ncp_client_secret

# Google Maps API (Places)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Google Gemini AI
GEMINI_API_KEY=your_gemini_api_key

# TMAP API (SK OpenAPI - Directions)
TMAP_API_KEY=your_tmap_api_key

# Naver Cloud OCR
NAVER_OCR_SECRET_KEY=your_naver_ocr_secret_key
NAVER_OCR_INVOKE_URL=your_naver_ocr_invoke_url

JWT_SECRET=your_jwt_secret_key

# Admin Secret Key (admin-backend webhook authentication)
# Must match the ADMIN_SECRET_KEY in admin-backend
ADMIN_SECRET_KEY=your-admin-secret-key-change-this

# Enable store photos in home list (increases API costs)
# Set to true for demo videos, false for production
ENABLE_STORE_PHOTOS=false
