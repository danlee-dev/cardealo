{
  "cli": {
    "version": ">= 13.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "http://localhost:5001",
        "EXPO_PUBLIC_ENABLE_LOCATION_DEBUG": "true",
        "EXPO_PUBLIC_ENABLE_TEST_LOGIN": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://cardealo-production.up.railway.app",
        "EXPO_PUBLIC_ENABLE_LOCATION_DEBUG": "false",
        "EXPO_PUBLIC_ENABLE_TEST_LOGIN": "false"
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://cardealo-production.up.railway.app",
        "EXPO_PUBLIC_ENABLE_LOCATION_DEBUG": "false",
        "EXPO_PUBLIC_ENABLE_TEST_LOGIN": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
